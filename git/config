[user]
	name = user.name
	email = user.email

[alias]
	ci = commit
    co = checkout
	st = status
	sh = stash
	br = branch
	su = submodule update --init
	la = log --graph --oneline --decorate --all
    cp = cherry-pick
    rp = rebase --reapply-cherry-picks
    rs = rebase --skip
	rc = rebase --continue
	ra = rebase --abort

